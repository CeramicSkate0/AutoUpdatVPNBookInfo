using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Net;
using System.Net.Http;
using System.Text.RegularExpressions;

namespace VPNBookAutoGETPassword
{
    class Program
    {
        static void Main(string[] args)
        {
            try
            {
                WebClient wr = new WebClient();
                List<string> matchs = new List<string>();
                string page = wr.DownloadString("http://www.vpnbook.com/freevpn");
                string name = "<li>Password:   <strong>(.*?)</strong></li>";
                foreach (Match match in Regex.Matches(page, name, RegexOptions.Singleline))
                {
                    matchs.Add(match.Groups[1].Value);
                    Console.WriteLine(match.Groups[1].Value);
                }
                wr.Dispose();
            }
            catch
            {
                Console.WriteLine("Can not view the site :(");
            }
        }
    }
}
